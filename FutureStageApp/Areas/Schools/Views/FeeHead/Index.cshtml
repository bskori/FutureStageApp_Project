@model IEnumerable<FeeHead>

    <div class="row">
        <div class="col-md-12">
            <br />
            <br />
            <h1>FeeHead List</h1>
            <p>
                <a asp-action="Create" class="btn btn-primary">Create</a>
            </p>
            <table class="table table-striped table-hover">
                <tr class="thead-dark">
                    <th>FeeHead ID</th>
                    <th>FeeHead Title</th>
                    <th>FeeHead Description</th>
                    <th></th>
                </tr>
                @foreach(var temp in Model)
            {
                <tr>
                    <td>@temp.FeeHeadID</td>
                    <td>@temp.FeeHeadTitle</td>
                    <td>@temp.FeeHeadDesc</td>
                    
                    <td>
                        <a class="btn btn-success"  asp-action="Edit" asp-route-id="@temp.FeeHeadID">Edit</a>
                     
                        <a class="btn btn-danger" asp-action="Delete" asp-route-id="@temp.FeeHeadID">Delete</a>
                    </td>
                </tr>
            }
            </table>
        </div>
    </div>




